; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Iso",
"language": "GDScript",
"path": "res://source/map/Iso.gd"
}, {
"base": "Reference",
"class": "Loader",
"language": "GDScript",
"path": "res://source/global/Loader.gd"
}, {
"base": "Resource",
"class": "MapData",
"language": "GDScript",
"path": "res://source/data/MapData.gd"
} ]
_global_script_class_icons={
"Iso": "",
"Loader": "",
"MapData": ""
}

[application]

config/name="MightyTower"
run/main_scene="res://source/menu/TitleScreen.tscn"
config/icon="res://icon.png"

[autoload]

Scene="*res://source/global/Scene.gd"
Data="*res://source/global/Data.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

environment/default_environment="res://default_env.tres"
